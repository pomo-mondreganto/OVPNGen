mode p2p

port {{ config.JURY_PORT }}

dev jury
dev-type tun
proto {{ 'udp' if config.PROTOCOL == 'udp' else 'tcp-server'}}

txqueuelen 1000
fragment 0
mssfix 0
tun-mtu 6000
cipher bf-cbc

ifconfig 10.10.10.1 10.10.10.10

keepalive 10 60

ping-timer-rem

persist-tun
persist-key

<secret>
{{ static_key }}
</secret>
